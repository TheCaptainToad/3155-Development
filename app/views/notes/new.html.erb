<h1><center>Create Your Note</center></h1>
<center>
<%= form_for :note, url: notes_path do |f| %>
    <p>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </p>
    
    <p>
        <%= f.label :text %><br>
        <%= f.text_area :title %>
    </p>
    
    <p>
        <%= f.submit %>
    </p>
</center>
<% end %>